{% extends 'layout.html.twig' %}

{% block contenido %}
    <h1>Semillero</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ semillero.id }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ semillero.nombre }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('semillero_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('semillero_edit', { 'id': semillero.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
